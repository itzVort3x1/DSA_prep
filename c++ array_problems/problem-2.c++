//------------------------------------
//      Reverse the elements in the array.
//      Ex: INPUT: arr={2,5,6,8,12}
//          OUTPUT: 12, 8, 6, 5, 2
//------------------------------------

#include<iostream>
using namespace std;

int main(){
    int arr[5] = {2,5,6,8,12};

    for(int i=0; i>=0; i--){
        cout<<arr[i]<<" ";
    }
}